#include <bits/stdc++.h>
using namespace std;

int main()
{
    
    int t;
    cin>>t;
    while(t--)
    {
        int A,B,C;
        cin>>A>>B>>C;
        int z = max(A,max(B,C));
        if(z==A)
        {
            cout<<"Alice"<<endl;
        }
        else if(z==B)
        {
            cout<<"Bob"<<endl;
        }
        else
        {
            cout<<"Charlie"<<endl;
        }
        
    }
    
    return 0;
}
